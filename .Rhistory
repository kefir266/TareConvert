tare <- function(...) {
files <- list.files(...,pattern = "*.csv",full.names = TRUE)
for (name in files){
message(name)
tab <- read.csv2(name)
tab <- as.matrix(tab)
tab <- rbind(tab[,c(1,2,3)],tab[,c(4,5,6)],tab[,c(7,8,9)])
tab <- tab[complete.cases(tab),]
tab <- tab[order(tab[,1]),]
write.csv(tab, file=paste0(name,".txt"),row.names = FALSE,col.names = FALSE)
}
}
